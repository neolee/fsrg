<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <servlet>
    <servlet-name>service</servlet-name>
    <servlet-class>com.foresee.fsr.gallery.server.ExampleServiceImpl</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>fileservice</servlet-name>
    <servlet-class>com.foresee.fsr.gallery.server.FileServiceImpl</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>xmltree</servlet-name>
    <servlet-class>com.foresee.fsr.gallery.server.XmlTreeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>xmltree</servlet-name>
    <url-pattern>/com.foresee.fsr.gallery.explorer.Explorer/xmltreeloader</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>xmltree</servlet-name>
    <url-pattern>/com.foresee.fsr.gallery.Gallery/xmltreeloader</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>service</servlet-name>
    <url-pattern>/com.foresee.fsr.gallery.explorer.Explorer/service</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>service</servlet-name>
    <url-pattern>/com.foresee.fsr.gallery.Gallery/service</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>fileservice</servlet-name>
    <url-pattern>/com.foresee.fsr.gallery.explorer.Explorer/fileservice</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>fileservice</servlet-name>
    <url-pattern>/com.foresee.fsr.gallery.Gallery/fileservice</url-pattern>
  </servlet-mapping>
</web-app>